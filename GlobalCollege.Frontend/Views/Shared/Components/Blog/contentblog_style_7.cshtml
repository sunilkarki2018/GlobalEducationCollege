@model BlogInformation
@{
    Layout = "";

}

<section class="our-mission">
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <h2 class="section-title"> @Model.Title</h2>
                <p>
                    @Html.Raw(Model.DetailDescription)
                </p>
            </div>
            <div class="col-md-6">
                <img src="@Model.ThumbnailImageLink" alt="mission">
            </div>
        </div>
    </div>
</section>



